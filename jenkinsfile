pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Hello Build'
                sleep 5
            }
        }
        stage('Test') {
            steps {
                echo 'Hello Test'
                sleep 4
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello deploy'
            }
        }
        stage('Monitor') {
            steps {
                echo 'Hello Monitor'
            }
        }
    }
}
